<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#dbeafe;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#bfdbfe;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="human" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="ai" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Title -->
  <text x="400" y="70" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="#1e40af">
    Human-AI Symbiosis
  </text>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#1e40af">
    Collaborative Intelligence Partnership
  </text>
  
  <!-- Human Figure -->
  <circle cx="250" cy="250" r="60" fill="url(#human)" opacity="0.3"/>
  <circle cx="250" cy="220" r="25" fill="#3b82f6"/>
  <rect x="225" y="245" width="50" height="60" fill="#3b82f6" rx="10"/>
  <text x="250" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1e40af">
    Human
  </text>
  <text x="250" y="360" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#1e40af">
    Creativity • Ethics
  </text>
  <text x="250" y="375" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#1e40af">
    Intuition • Empathy
  </text>
  
  <!-- AI Figure -->
  <circle cx="550" cy="250" r="60" fill="url(#ai)" opacity="0.3"/>
  <rect x="525" y="225" width="50" height="50" fill="#10b981" rx="5"/>
  <!-- Circuit pattern inside AI -->
  <line x1="535" y1="235" x2="565" y2="235" stroke="#059669" stroke-width="2"/>
  <line x1="535" y1="245" x2="565" y2="245" stroke="#059669" stroke-width="2"/>
  <line x1="535" y1="255" x2="565" y2="255" stroke="#059669" stroke-width="2"/>
  <line x1="535" y1="265" x2="565" y2="265" stroke="#059669" stroke-width="2"/>
  <circle cx="540" cy="240" r="2" fill="#059669"/>
  <circle cx="560" cy="250" r="2" fill="#059669"/>
  <circle cx="545" cy="260" r="2" fill="#059669"/>
  
  <text x="550" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#059669">
    AI Agent
  </text>
  <text x="550" y="360" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#059669">
    Processing • Analysis
  </text>
  <text x="550" y="375" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#059669">
    Memory • Automation
  </text>
  
  <!-- Connection/Collaboration Indicators -->
  <path d="M 310 250 Q 400 200 490 250" stroke="#6366f1" stroke-width="4" fill="none" opacity="0.7"/>
  <path d="M 310 250 Q 400 300 490 250" stroke="#6366f1" stroke-width="4" fill="none" opacity="0.7"/>
  
  <!-- Collaboration symbols -->
  <circle cx="400" cy="225" r="8" fill="#6366f1"/>
  <text x="400" y="230" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="white">+</text>
  
  <circle cx="400" cy="275" r="8" fill="#6366f1"/>
  <text x="400" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="white">+</text>
  
  <!-- Partnership Benefits -->
  <rect x="300" y="400" width="200" height="120" fill="#6366f1" opacity="0.1" rx="10"/>
  <text x="400" y="425" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#4338ca">
    Symbiotic Benefits
  </text>
  
  <text x="320" y="450" font-family="Arial, sans-serif" font-size="12" fill="#4338ca">
    ✓ Amplified Creativity
  </text>
  <text x="320" y="470" font-family="Arial, sans-serif" font-size="12" fill="#4338ca">
    ✓ Enhanced Decision Making
  </text>
  <text x="320" y="490" font-family="Arial, sans-serif" font-size="12" fill="#4338ca">
    ✓ Preserved Human Agency
  </text>
  <text x="320" y="510" font-family="Arial, sans-serif" font-size="12" fill="#4338ca">
    ✓ Mutual Learning & Growth
  </text>
  
  <!-- Individual Strengths -->
  <rect x="150" y="150" width="120" height="60" fill="#3b82f6" opacity="0.1" rx="5"/>
  <text x="210" y="170" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#1e40af">
    Human Strengths
  </text>
  <text x="160" y="190" font-family="Arial, sans-serif" font-size="10" fill="#1e40af">
    • Strategic Thinking
  </text>
  <text x="160" y="205" font-family="Arial, sans-serif" font-size="10" fill="#1e40af">
    • Emotional Intelligence
  </text>
  
  <rect x="530" y="150" width="120" height="60" fill="#10b981" opacity="0.1" rx="5"/>
  <text x="590" y="170" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#059669">
    AI Strengths
  </text>
  <text x="540" y="190" font-family="Arial, sans-serif" font-size="10" fill="#059669">
    • Data Processing
  </text>
  <text x="540" y="205" font-family="Arial, sans-serif" font-size="10" fill="#059669">
    • Pattern Recognition
  </text>
  
  <!-- Bottom tagline -->
  <text x="400" y="560" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#4338ca">
    Together We Achieve More
  </text>
</svg>