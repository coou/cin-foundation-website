<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fef3c7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fde68a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="sandbox" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#f59e0b;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#d97706;stop-opacity:0.3" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Title -->
  <text x="400" y="70" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="#92400e">
    Ethical Sandboxing
  </text>
  <text x="400" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#78350f">
    Safe Spaces for Social Experimentation
  </text>
  
  <!-- Main Sandbox Container -->
  <rect x="150" y="150" width="500" height="300" fill="url(#sandbox)" stroke="#d97706" stroke-width="3" stroke-dasharray="10,5" rx="20"/>
  
  <!-- Sandbox Label -->
  <text x="400" y="140" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#92400e">
    CONTROLLED ENVIRONMENT
  </text>
  
  <!-- Experiment Elements Inside Sandbox -->
  <!-- People icons -->
  <circle cx="250" cy="220" r="20" fill="#f59e0b" opacity="0.7"/>
  <circle cx="250" cy="210" r="8" fill="#92400e"/>
  <rect x="242" y="220" width="16" height="20" fill="#92400e" rx="3"/>
  
  <circle cx="350" cy="250" r="20" fill="#f59e0b" opacity="0.7"/>
  <circle cx="350" cy="240" r="8" fill="#92400e"/>
  <rect x="342" y="250" width="16" height="20" fill="#92400e" rx="3"/>
  
  <circle cx="450" cy="220" r="20" fill="#f59e0b" opacity="0.7"/>
  <circle cx="450" cy="210" r="8" fill="#92400e"/>
  <rect x="442" y="220" width="16" height="20" fill="#92400e" rx="3"/>
  
  <circle cx="550" cy="250" r="20" fill="#f59e0b" opacity="0.7"/>
  <circle cx="550" cy="240" r="8" fill="#92400e"/>
  <rect x="542" y="250" width="16" height="20" fill="#92400e" rx="3"/>
  
  <!-- Interaction lines -->
  <line x1="270" y1="230" x2="330" y2="250" stroke="#d97706" stroke-width="2" opacity="0.6"/>
  <line x1="370" y1="250" x2="430" y2="230" stroke="#d97706" stroke-width="2" opacity="0.6"/>
  <line x1="470" y1="230" x2="530" y2="250" stroke="#d97706" stroke-width="2" opacity="0.6"/>
  
  <!-- Experiment indicators -->
  <rect x="200" y="320" width="80" height="30" fill="#fbbf24" opacity="0.5" rx="5"/>
  <text x="240" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#92400e">
    New Economy
  </text>
  
  <rect x="320" y="320" width="80" height="30" fill="#fbbf24" opacity="0.5" rx="5"/>
  <text x="360" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#92400e">
    Governance
  </text>
  
  <rect x="440" y="320" width="80" height="30" fill="#fbbf24" opacity="0.5" rx="5"/>
  <text x="480" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#92400e">
    Collaboration
  </text>
  
  <!-- Safety Boundaries -->
  <text x="180" y="180" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Informed Consent
  </text>
  <text x="180" y="200" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Reversible Changes
  </text>
  <text x="180" y="420" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Bounded Impact
  </text>
  <text x="180" y="440" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Transparent Monitoring
  </text>
  
  <text x="520" y="180" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Exit Rights
  </text>
  <text x="520" y="200" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Ethical Oversight
  </text>
  <text x="520" y="420" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Learning Focus
  </text>
  <text x="520" y="440" font-family="Arial, sans-serif" font-size="12" fill="#92400e">
    ✓ Gradual Scaling
  </text>
  
  <!-- Outside world indication -->
  <text x="100" y="300" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#6b7280">
    Existing
    <tspan x="100" dy="20">Systems</tspan>
  </text>
  
  <text x="700" y="300" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#6b7280">
    Future
    <tspan x="700" dy="20">Adoption</tspan>
  </text>
  
  <!-- Arrows showing progression -->
  <path d="M 120 300 L 140 300 L 130 290 M 140 300 L 130 310" stroke="#6b7280" stroke-width="2" fill="none"/>
  <path d="M 660 300 L 680 300 L 670 290 M 680 300 L 670 310" stroke="#6b7280" stroke-width="2" fill="none"/>
  
  <!-- Bottom text -->
  <text x="400" y="520" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#78350f">
    Testing Innovation Safely
  </text>
</svg>